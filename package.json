{
	"name": "@absolutejs/docs",
	"version": "0.0.1",
	"description": "Documentation for AbsoluteJS and its associated packages.",
	"repository": {
		"type": "git",
		"url": "https://github.com/absolutejs/absolutejs.git"
	},
	"main": "./dist/index.js",
	"types": "./dist/src/index.d.ts",
	"license": "CC BY-NC 4.0",
	"author": "Alex Kahn",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,json,mjs,md}\"",
		"dev": "bun run --watch src/backend/server.ts",
		"lint": "eslint ./src ./eslint.config.mjs",
		"typecheck": "bun run tsc --noEmit",
		"db:studio": "drizzle-kit studio",
		"db:push": "drizzle-kit push",
		"seed:providers": "bun run db/scripts/seedProviders.ts",
		"update:provider-status": "bun run db/scripts/updateProviderStatus.ts",
		"reset:provider-statuses": "bun run db/scripts/resetAllProviderStatuses.ts",
		"get:providers:by-status": "bun run db/scripts/getProvidersByStatus.ts"
	},
	"dependencies": {
		"@absolutejs/absolute": "0.12.3",
		"@absolutejs/auth": "0.14.3",
		"@elysiajs/eden": "1.2.0",
		"@elysiajs/static": "1.3.0",
		"@neondatabase/serverless": "1.0.0",
		"@react-spring/web": "9.7.5",
		"@tanstack/react-query": "5.79.0",
		"drizzle-orm": "0.41.0",
		"elysia": "1.2.25",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-icons": "5.5.0",
		"react-syntax-highlighter": "15.6.1",
		"zustand": "5.0.5"
	},
	"devDependencies": {
		"@types/react-syntax-highlighter": "15.5.13",
		"@stylistic/eslint-plugin-ts": "4.2.0",
		"@tanstack/eslint-plugin-query": "5.78.0",
		"@types/bun": "1.2.12",
		"@types/react": "18.3.12",
		"@types/react-dom": "18.3.1",
		"drizzle-kit": "0.30.6",
		"eslint": "9.26.0",
		"eslint-plugin-absolute": "0.1.4",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-compiler": "19.1.0-rc.1",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-security": "3.0.1",
		"prettier": "3.5.3",
		"typescript": "5.8.3",
		"typescript-eslint": "8.32.0"
	}
}
